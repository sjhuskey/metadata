<rdf:RDF xml:lang="en" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:madsrdf="http://www.loc.gov/mads/rdf/v1#"
  xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:ri="http://id.loc.gov/ontologies/RecordInfo#">
  <!-- Value of rdf:about should be field_dll_page_link -->
  <madsrdf:PersonalName rdf:about="field_dll_page_link">
    <rdf:type rdf:resource="http://www.loc.gov/mads/rdf/v1#Authority"/>
    <madsrdf:authoritativeLabel xml:lang="en"
      ><!-- Value should be field_authorized_name --></madsrdf:authoritativeLabel>
    <!-- Latin variant of author's name (field_author_name_latin) -->
    <madsrdf:hasVariant>
      <madsrdf:PersonalName>
        <rdf:type rdf:resource="http://www.loc.gov/mads/rdf/v1#Variant"/>
        <madsrdf:variantLabel xml:lang="lat">field_author_name_latin</madsrdf:variantLabel>
      </madsrdf:PersonalName>
    </madsrdf:hasVariant>
    <!-- English variant of author's name (field_author_name_english) -->
    <madsrdf:hasVariant>
      <madsrdf:PersonalName>
        <rdf:type rdf:resource="http://www.loc.gov/mads/rdf/v1#Variant"/>
        <madsrdf:variantLabel xml:lang="en">field_author_name_english</madsrdf:variantLabel>
      </madsrdf:PersonalName>
    </madsrdf:hasVariant>
    <!-- Native language variant of author's name (field_author_name_native_languag) -->
    <madsrdf:hasVariant>
      <madsrdf:PersonalName>
        <rdf:type rdf:resource="http://www.loc.gov/mads/rdf/v1#Variant"/>
        <madsrdf:variantLabel xml:lang="en">field_author_name_native_languag</madsrdf:variantLabel>
      </madsrdf:PersonalName>
    </madsrdf:hasVariant>
    <!-- Also known as (field_also_known_as) -->
    <madsrdf:hasVariant>
      <madsrdf:PersonalName>
        <rdf:type rdf:resource="http://www.loc.gov/mads/rdf/v1#Variant"/>
        <madsrdf:variantLabel xml:lang="en">field_also_known_as</madsrdf:variantLabel>
      </madsrdf:PersonalName>
    </madsrdf:hasVariant>
    <madsrdf:hasVariant>
      <madsrdf:PersonalName>
        <rdf:type rdf:resource="http://www.loc.gov/mads/rdf/v1#Variant"/>
        <madsrdf:variantLabel xml:lang="es">field_bne_url:title</madsrdf:variantLabel>
      </madsrdf:PersonalName>
    </madsrdf:hasVariant>
    <madsrdf:hasVariant>
      <madsrdf:PersonalName>
        <rdf:type rdf:resource="http://www.loc.gov/mads/rdf/v1#Variant"/>
        <madsrdf:variantLabel xml:lang="fr">field_bnf_url:title</madsrdf:variantLabel>
      </madsrdf:PersonalName>
    </madsrdf:hasVariant>
    <madsrdf:hasVariant>
      <madsrdf:PersonalName>
        <rdf:type rdf:resource="http://www.loc.gov/mads/rdf/v1#Variant"/>
        <madsrdf:variantLabel xml:lang="de">field_dnb_url:title</madsrdf:variantLabel>
      </madsrdf:PersonalName>
    </madsrdf:hasVariant>
    <madsrdf:hasVariant>
      <madsrdf:PersonalName>
        <rdf:type rdf:resource="http://www.loc.gov/mads/rdf/v1#Variant"/>
        <madsrdf:variantLabel xml:lang="it">field_iccu_url:title</madsrdf:variantLabel>
      </madsrdf:PersonalName>
    </madsrdf:hasVariant>
    <madsrdf:hasVariant>
      <madsrdf:PersonalName>
        <rdf:type rdf:resource="http://www.loc.gov/mads/rdf/v1#Variant"/>
        <madsrdf:variantLabel xml:lang="en">field_isni_url:title</madsrdf:variantLabel>
      </madsrdf:PersonalName>
    </madsrdf:hasVariant>
    <madsrdf:birthDate>field_author_birth_date</madsrdf:birthDate>
    <madsrdf:deathDate>field_author_death_date</madsrdf:deathDate>
    <madsrdf:temporal>field_floruit_active</madsrdf:temporal>
    <madsrdf:temporal>field_time_period_authorities</madsrdf:temporal>
    <!-- URLs/URIs -->
    <!-- VIAF (field_viaf_source) -->
    <madsrdf:hasExactExternalAuthority rdf:resource="field_viaf_source"/>
    <!-- https://lccn.loc.gov (field_locsource) -->
    <madsrdf:hasExactExternalAuthority rdf:resource="field_locsource"/>
    <!-- http://id.loc.gov/authorities/names/ (field_lofc_uri) -->
    <madsrdf:hasExactExternalAuthority rdf:resource="field_lofc_uri"/>
    <madsrdf:hasExactExternalAuthority rdf:resource="field_bne_url:url"/>
    <madsrdf:hasExactExternalAuthority rdf:resource="field_bnf_url:url"/>
    <madsrdf:hasExactExternalAuthority rdf:resource="field_dnb_url:url"/>
    <madsrdf:hasExactExternalAuthority rdf:resource="field_iccu_url:url"/>
    <madsrdf:hasExactExternalAuthority rdf:resource="field_isni_url:url"/>
    <madsrdf:hasExactExternalAuthority rdf:resource="field_wikidata_url:url"/>
    <madsrdf:hasExactExternalAuthority rdf:resource="field_wikipedia:url"/>
    <madsrdf:hasExactExternalAuthority rdf:resource="field_worldcat_identity:url"/>
    <!-- ID's -->
    <!-- DLL -->
    <madsrdf:hasIdentifier>
      <madsrdf:Identifier>
        <madsrdf:idScheme>DLL</madsrdf:idScheme>
        <!-- Value: field_dll_identifier -->
        <madsrdf:idValue>field_dll_identifier</madsrdf:idValue>
      </madsrdf:Identifier>
    </madsrdf:hasIdentifier>
    <!-- VIAF -->
    <madsrdf:hasIdentifier>
      <madsrdf:Identifier>
        <madsrdf:idScheme>VIAF</madsrdf:idScheme>
        <!-- Value:  field_viaf_id-->
        <madsrdf:idValue>field_viaf_id</madsrdf:idValue>
      </madsrdf:Identifier>
    </madsrdf:hasIdentifier>
    <!-- LOC -->
    <madsrdf:hasIdentifier>
      <madsrdf:Identifier>
        <madsrdf:idScheme>LOC</madsrdf:idScheme>
        <!-- Value: field_loc_id -->
        <madsrdf:idValue>field_loc_id</madsrdf:idValue>
      </madsrdf:Identifier>
    </madsrdf:hasIdentifier>
    <!-- STOA -->
    <madsrdf:hasIdentifier>
      <madsrdf:Identifier>
        <madsrdf:idScheme>STOA</madsrdf:idScheme>
        <!-- Value: field_stoa_number -->
        <madsrdf:idValue>field_stoa_number</madsrdf:idValue>
      </madsrdf:Identifier>
    </madsrdf:hasIdentifier>
    <!-- PHI -->
    <madsrdf:hasIdentifier>
      <madsrdf:Identifier>
        <madsrdf:idScheme>PHI</madsrdf:idScheme>
        <!-- Value: field_phi_number -->
        <madsrdf:idValue>field_phi_number</madsrdf:idValue>
      </madsrdf:Identifier>
    </madsrdf:hasIdentifier>
    <!-- CTS -->
    <madsrdf:hasIdentifier>
      <madsrdf:Identifier>
        <madsrdf:idScheme>CTS</madsrdf:idScheme>
        <!-- Value: field_cts_urn -->
        <madsrdf:idValue>field_cts_urn</madsrdf:idValue>
      </madsrdf:Identifier>
    </madsrdf:hasIdentifier>
    <madsrdf:adminMetadata>
      <ri:RecordInfo>
        <ri:creationDate>[node:created:short]</ri:creationDate>
        <ri:recordIdentifier>[node:feeds-item-guid]</ri:recordIdentifier>
      </ri:RecordInfo>
    </madsrdf:adminMetadata>
  </madsrdf:PersonalName>
</rdf:RDF>
